{
  "version": 2,
  "buildCommand": "cd backend && pnpm install && pnpm build && cd ../frontend && pnpm install && pnpm build",
  "installCommand": "echo 'skip'",
  "outputDirectory": "frontend/.output/public",
  "rewrites": [
    {
      "source": "/api/resume/:path*",
      "destination": "/api/resume-handler"
    },
    {
      "source": "/api/user-auth/:path*",
      "destination": "/api/user-auth-handler"
    }
  ]
}
